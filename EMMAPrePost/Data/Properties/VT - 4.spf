<TYPE>  4

T - temperature [K]
e - strain [-]
ee- strain rate [1/s]
S - stress [MPa]
t - time [s]


***************************************************
STESS/STRAIN + DRX
***************************************************

S  = (1-X)*Srv*(1-exp(-a_drv*e))^n_drv + X*Srx

<R>	8.31
<a_drv>	7.0 
<n_drv>	0.4 

X  = 1-exp(-a_drx*max(0, (e-e0)/e0)^n_drx)

<a_drx>	0.338
<n_drx>	1.5

Srv= A_rv*[ee*exp(Q_rv/(R*T))]^m_rv

<A_rv>	1.5
<Q_rv>	312000
<m_rv>	0.4

Srx= A_rx*[ee*exp(Q_rx/(R*T))]^m_rx

<A_rx>	1.5
<Q_rx>	312000
<m_rx>	0.4

e0 = A_e*D0^m_ed*[ee*exp(Q_e/(R*T))]^m_ee

<A_e>	4.4E-4
<m_ed>	0.18
<Q_e>	312000
<m_ee>	0.15

Dd = A_drx*[ee*exp(Q_dd/(R*T))]^m_drx

<A_drx>	1.4E3
<m_drx>	-0.16
<Q_dd>	312000

***************************************************
STATIC RECRYSTALLYZATION
***************************************************

Xs = 1 - exp(-0.693*(t/ts50))^n_srx

<n_srx>	0.95

ts50 = A_s*D0^p1*e^p2*ee^p3*exp(Q_srx/(R*T))

<A_s>	9.3E-13
<p1>	2
<p2>	-4
<p3>	-0.0
<Q_srx>	230000

Ds = C0 + C1*D0^q1*e^q2*ee^q3*exp(-Q_ds/(R*T))

<C0>	0.0
<C1>	4.54
<q1>	0.67
<q2>	-0.53
<q3>	-0.1
<Q_ds>	15000


***************************************************
METADYNAMIC RECRYSTALLYZATION
***************************************************

Xm = 1 - exp(-0.693*(t/tm50))^n_mrx

<n_mrx>	1.3

tm50 = A_m*[ee*exp(Q_mz/(R*T))]^p_m*exp(Q_mrx/(R*T))

<A_m>	1.12
<Q_mz>	312000
<p_m>	-0.8
<Q_mrx>	230000

Dm = C_m*[ee*exp(Q_mz/(R*T))]^m_mrx

<C_m>	2.3E4
<m_mrx>	-0.16

***************************************************
GRAIN GROWTH
***************************************************
D = (D(0)^2 + t*10^(A1-B1/T))^0.5       | for t<t_gg1
D = (D(t_gg1)^2 + t*10^(A2-B2/T))^0.5   | for t>=t_gg1

<t_gg1>	20
<A1>	7.1
<B1>	7180
<A2>	9.5
<B2>	10920

