parameter h (50);
parameter w (20);
parameter h1 (20);
parameter w1 (10);

point p1 (w - w1, 0);
point p2 (w - w1, h - h1);
point p3 (0, h - h1);
point p4 (0, h);
point p5 (w, h);
point p6 (w, 0);

line l1 (p1, p2);
line l2 (p2, p3);
line l3 (p3, p4);
line l4 (p4, p5);
line l5 (p5, p6);
line l6 (p6, p1);
line l7 (p2, p5);

contour c1 (l1, l2, l3, l4, l5, l6);
contour c2 (l2, l3, l4, l7);
contour c3 (l1, l7, l5, l6);